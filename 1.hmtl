function tableRender(strDate,selectType) {
$('#loading').show();
$('#reports, #tbl-credit, #tbl-generic').hide();
var tblIdList = ['#credit-table','#report-table'];
$.each(tblIdList, function(idx, tblId) {
if ( $.fn.DataTable.isDataTable( tblId) ) {
$(tblId).DataTable().destroy();
}
$(tblId).empty();
});
$('#creditTitle>h1').empty();
$('#creditNotes').empty().hide();
$('#title>h1').empty();
$('#notes').empty().hide();
if(typeof(strDate)=='undefined') strDate = '';
if(typeof(selectType)=='undefined') selectType = '';
$.getJSON(API+'?response=json&date='+strDate+'&selectType='+selectType, function(J) {
if(J.stat == 'OK') {
if(/^(\d{4})(\d{2})(\d{2})$/.test(J.date))
bxds.set('d1', new Date(parseInt(RegExp.$1,10),parseInt(RegExp.$2,10)-1,parseInt(RegExp.$3,10)));
cherkBFIHBU(J.date, J.date);
$('select[name="selectType"]').val(J.selectType);//分類項目
if(J.creditTitle != null && J.creditTitle.length > 0) $('#creditTitle>h1').text(J.creditTitle);
$('#title>h1').text(J.title);
if('creditNotes' in J && J.creditNotes.length > 0) {
var i, html = '<b>說明：</b><ol>';
for (i=0;i<J.creditNotes.length;i++) html += '<li>'+(J.creditNotes[i].replace(/\n/g,'<br>'))+'</li>';
html += '</ol>';
$('#creditNotes').html(html).show();
}
if('notes' in J && J.notes.length > 0) {
var i, html = '<b>說明：</b><ol>';
for (i=0;i<J.notes.length;i++) html += '<li>'+(J.notes[i].replace(/\n/g,'<br>'))+'</li>';
html += '</ol>';
$('#notes').html(html).show();
}
$('#reports a.html').attr('href',API+'?response=html&date='+J.date+'&selectType='+J.selectType);
$('#reports a.csv').attr('href',API+'?response=csv&date='+J.date+'&selectType='+J.selectType);
var aligns = [
['center','center','center','center','center',
'center','center','center','center','center',
'center','center','center','center','center',
'center'],
['center','center','right','right','right',
'right','right','right','right','right',
'right','right','right','right','center',
'center']
];
var creditAligns = [
['center','center','center','center','center','center'],
['center','right','right','right','right','right']
];
if(J.creditFields.length>0) {
var tblCredit = $('#credit-table').DataTable({lengthMenu: [ [10, 25, 50, 100, -1], [10, 25, 50, 100,"全部"] ],aaSorting: [],
fixedHeader: true,
language: {url:_app.rsrc+'/lib/table/zh.json'},
paging:   false,
ordering: true,
info:     false,
searching: false,
destroy: true,
columns: dateTableColumns(J.creditFields,creditAligns),
data: J.creditList
});
}
if(J.fields.length>0) {
var tbl = $('#report-table')
.TableHead(J.fields,J.groups)
.DataTable({lengthMenu: [ [10, 25, 50, 100, -1], [10, 25, 50, 100,"全部"] ],
aaSorting: [],
fixedHeader: true,
language: {url:_app.rsrc+'/lib/table/zh.json'},
paging:   true,
ordering: true,
info:     false,
searching: false,
destroy: true,
columns: dateTableColumns(J.fields,aligns),
data: J.data,
pagingType: "simple_numbers"
});
}
$('#loading').hide();
$('#reports').show();
if(J.creditFields.length>0) {
$("#tbl-credit").show();
}
if(J.fields.length>0) {
$("#tbl-generic").show();
}
} else {
cherkBFIHBU('', '');
$('#reports').hide();
$('#loading').css('display','none');
$('#result-message').text(J.stat).show();
}
});
}
function cherkBFIHBU(startDate, endDate) {
$.getJSON(BFIHBU_API+'?response=json&startDate='+startDate+'&endDate='+endDate, function(J) {
if(J.stat == 'OK') {
/^(\d{4})(\d{2})(\d{2})$/.test(J.params.startDate);
var notes1 = "&nbsp;";
var notes2 = "<br><font color='red'><b>" + (parseInt(RegExp.$1,10) - 1911) + "年" + parseInt(RegExp.$2,10) + "月" + parseInt(RegExp.$3,10) + "日臨時新增變更交易有價證券 <a href='/zh/page/trading/exchange/BFIHBU.html?date=" + J.params.startDate + "' target='_blank'>明細查詢</a></b></font>";
if (J.data.length > 0) {
$(".BFIHBU").html(notes2);
} else {
$(".BFIHBU").html(notes1);
}
}
});
}
